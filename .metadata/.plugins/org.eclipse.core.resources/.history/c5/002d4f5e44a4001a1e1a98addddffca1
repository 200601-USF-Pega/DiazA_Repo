package holder;
//POJO
public class Hero {
	//attributes
	//private makes this safe from modification, encapsulates
	private String name;
	private String specialMove;
	private int healthLavel; 
	private boolean isAlive; 
	
	//constructor
	public Hero() {
		
	}

	public Hero(String name, String specialMove, int healthLavel, boolean isAlive) {
		super();
		//calls parent
		this.name = name;
		this.specialMove = specialMove;
		this.healthLavel = healthLavel;
		this.isAlive = isAlive;
	}
	
}
